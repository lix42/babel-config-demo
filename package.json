{
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-env": "^1.1.10",
    "babel-preset-es2015": "^6.3.13"
  },
  "dependencies": {
    "babel-runtime": "^6.3.13",
    "core-js": "^2.4.1"
  },
  "scripts": {
    "build": "babel es6 --out-dir es5",
    "watch": "babel es6 --out-dir es5 --watch"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current",
            "browsers": "last 1 versions"
          },
          "modules": false,
          "useBuiltIns": true,
          "debug": true
        }
      ]
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "helpers": true,
          "polyfill": false,
          "regenerator": false
        }
      ]
    ]
  }
}